{
	"expo": {
		"name": "boop-rescues",
		"slug": "boop-rescues",
		"scheme": "booprescues",
		"icon": "./src/assets/icon.png",
		"version": "1.0.0",
		"orientation": "portrait",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": false
		},
		"android": {
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": true
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./src/assets/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		}
	}
}
