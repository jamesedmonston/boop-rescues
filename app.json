{
	"expo": {
		"name": "Boop Rescues",
		"slug": "boop-rescues",
		"scheme": "booprescues",
		"icon": "./src/assets/icon.png",
		"version": "1.0.0",
		"orientation": "portrait",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "com.ethercreative.boop-rescues"
		},
		"android": {
			"edgeToEdgeEnabled": true
		},
		"plugins": [
			"expo-router",
			"expo-dev-client",
			["expo-splash-screen", { "backgroundColor": "#7D5CFF" }],
			"react-native-edge-to-edge"
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		}
	}
}
